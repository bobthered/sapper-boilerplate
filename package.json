{
  "name": "sapper-boilerplate",
  "description": "sapper-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "build": "npm run tw:build && npm run sapper:build",
    "dev": "concurrently \"npm run sapper:dev\" \"npm run tw:dev\"",
    "export": "sapper export --legacy",
    "sapper:build": "sapper build",
    "sapper:dev": "sapper dev",
    "start": "node __sapper__/build",
    "tw:build": "postcss src/css/tailwind.css -o public/css/tailwind.css",
    "tw:dev": "postcss src/css/tailwind.css -o public/css/tailwind.css -w --verbose"
  },
  "dependencies": {
    "@bobthered/svelte-button": "^1.0.6",
    "@bobthered/svelte-heroicons": "^1.0.8",
    "compression": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@bobthered/tailwindcss-palette-generator": "^2.0.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.4.0",
    "@rollup/plugin-url": "^5.0.0",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.0.0",
    "cssnano": "^4.1.10",
    "express": "^4.17.1",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "sirv": "^1.0.11",
    "svelte": "^3.17.3",
    "tailwindcss": "^2.0.4"
  }
}
